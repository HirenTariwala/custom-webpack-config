{
  "extends": "eslint:recommended", // rules that report common problems, which have a check mark on https://eslint.org/docs/rules/
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module" // allow use to use import and export keyword
  },
  "env": {
    "node": true,
    "browser": true
  },
  "rules": {
    "no-console": "warn",
    "no-multiple-empty-lines": "error"
  }
}